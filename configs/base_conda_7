conda install -y pint  -c defaults -c conda-forge
conda install -y qgrid  -c defaults -c conda-forge
jupyter labextension install @jupyter-widgets/jupyterlab-manager
jupyter labextension install qgrid
conda install -y oct2py  -c defaults -c conda-forge
conda install -y celery  -c defaults -c conda-forge
